<h1>Login</h1>

<%= form_with method:'POST', url: 'instructors/authenticate' do |form| %>
    <div>
        <%= form.label :email %>
        <%= form.text_field :email %>
    </div>
    <div>
        <%= form.label :password %>
        <%= form.password_field :password %>
    </div>

    <%= form.submit %>
<% end %>